{
  "name": "discord-iris",
  "version": "0.0.4",
  "description": "A Discord bot library",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsup --dts-resolve"
  },
  "tsup": {
    "clean": true,
    "dts": true,
    "entryPoints": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "minify": true,
    "skipNodeModulesBundle": true,
    "sourcemap": true,
    "target": "es2021"
  },
  "author": "Joshua Castaneda",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@types/ws": "^8.2.2",
    "discord-api-types": "^0.26.1",
    "tsup": "^5.11.11",
    "typed-emitter": "^1.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "form-data": "^4.0.0",
    "ws": "^8.4.0"
  }
}
